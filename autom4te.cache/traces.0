       AM_CONDITIONAL(HAVE_RUNTIME_DIR, true)
       RUNTIME_DIR_LDFLAGS="-L$runtime_dir/$alt_abi_libdir"
       AC_MSG_RESULT(yes)
    else
       AM_CONDITIONAL(HAVE_RUNTIME_DIR, false)
       AC_MSG_RESULT(no)
    fi

